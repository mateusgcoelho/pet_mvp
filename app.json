{
  "expo": {
    "name": "pet_mvp",
    "slug": "pet_mvp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/icon1024white.png",
      "resizeMode": "contain",
      "backgroundColor": "#5949C1"
    },
    "androidStatusBar": {
      "backgroundColor": "#5949C1",
